[core]
	excludesfile = ~/.gitignore
[color]
	diff = auto
	status = auto
	branch = auto
[push]
	default = tracking
[alias]
    st = status
    di = diff --ignore-space-change
    co = checkout
    me = merge
    fh = fetch

    lg = log --graph --abbrev-commit --pretty=format:'%Cred%h%Creset %C(green)(%ci) %C(blue)%an%Creset: %s'
    jenkins = push origin +HEAD:jenkins

